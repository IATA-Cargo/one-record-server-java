               rdfs:label "changeRequest:patchRequest"@en .

               ###  https://onerecord.iata.org/api/ChangeRequest#requestingParty
               changeRequest:requestingParty rdf:type owl:ObjectProperty ;
                              rdfs:domain :ChangeRequest ;
                              rdfs:range :Branch ;
                              rdfs:comment "The party that has requested the change request"@en ;
                              rdfs:label "changeRequest:requestingParty"@en .

###  https://onerecord.iata.org/api/CompanyInformation#company
companyInformation:company rdf:type owl:ObjectProperty ;
               rdfs:domain :CompanyInformation ;
